<!DOCTYPE html>
<html lang="en">
    <%- include('../layout/head') %>
<body>
<%- include('../layout/header') %>
    <main>
        <div>
        <% results.forEach( book => { %>
            <img src="<%= book.image_url %>" alt="">
            <h3><%= book.title %></h3>
            <p><%= book.author %></p>
            <p><%= book.descriptions %></p>
        <% }) %>
        <p><%= count %></p>
    </div>
    </main>
    <%- include('../layout/footer')%>

</body>
</html>